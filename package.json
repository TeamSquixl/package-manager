{
  "name": "package-manager",
  "version": "0.0.2",
  "description": "A simple package debugger",
  "author": "Firebox Technology",
  "hosts": {
      "editor-framework": "^0.3.0"
  },
  "dependencies": {
      "ui-kit": "^0.0.3"
  },
  "main": "main.js",
  "menus": {
    "Developer/Package Manager": {
      "message": "package-manager:open"
    }
  },
  "panels": {
    "panel": {
      "frame": "panel/package-manager.html",
      "type": "dockable",
      "title": "Package Manager",
      "width": 500,
      "height": 800,
      "min-width": 500,
      "messages": [
          "package:loaded",
          "package:unloaded"
      ],
      "profiles": {
        "local": {
        }
      }
    }
  }
}
